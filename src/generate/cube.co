fs = require \fs

_ = require \../_
Cards = require \../../data/cards.json

module.exports = (numPacks, cube) ->
  names = _.choose numPacks * 15, cube
  cards = for name of names
    card = Cards[name]
    card.url = card.urls[Object.keys(card.urls).0]
    card
  packs = []

  for i from 0 til numPacks
    pack = []
    for j from 0 til 15
      pack.push cards.pop!
    packs.push pack

  packs
